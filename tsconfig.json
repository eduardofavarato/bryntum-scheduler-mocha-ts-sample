{
  "compilerOptions": {
    "outDir": "./build/transpiled",
    "rootDir": "src",
    "sourceMap": true,
    "noImplicitAny": false,
    "target": "es2019",
    "lib": [
      "es6",
      "es2016",
      "es2015.promise",
      "dom",
      "es2019"
    ],
    "strictNullChecks": true,
    "noImplicitThis": true,
    "jsx": "react",
    "module": "esnext",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": [
      "node"
    ],
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "@history": [
        "./src/history",
        "./build/transpiled/history"
      ],
      "@*": [
        "./src*",
        "./build/transpiled*"
      ],
      "@assets/*": [
        "./assets*",
        "./build/transpiled*"
      ],
      "@application/*": [
        "./src/application/*",
        "./src/application*",
        "./build/transpiled/application/*",
        "./build/transpiled/application*"
      ],
      "@contexts/*": [
        "./src/application/contexts/*",
        "./src/application/contexts*",
        "./build/transpiled/application/contexts/*",
        "./build/transpiled/application/contexts*"
      ],
      "@util/*": [
        "./src/util/*",
        "./src/util*",
        "./build/transpiled/util/*",
        "./build/transpiled/util*"
      ]
    },
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true
  },
  "awesomeTypescriptLoaderOptions": {
    "useCache": true
  },
  "include": [
    "src"
  ]
}
